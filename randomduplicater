{proc RandomDuplicater()
{	string $sels [] = `ls -sl`;

if (!size($sels) > 0)
{
	error("Must \"select\" an object!");
}
	string $obj[];
	string $objects[];
	int $duplicationAmount = 10;
	float $xPerametersLow = -10;
	float $xPerametersHigh = 10;
	float $yPerametersLow = -10;
	float $yPerametersHigh = 10;
	float $zPerametersLow = -10;
	float $zPerametersHigh = 10;
	float $xOffset = 0;
	float $yOffset = 20;
	float $zOffset = 0;
	
	
	
	for($i=0; $i<$duplicationAmount; $i++)
	{
		$obj = `duplicate`;
		$objects [size($objects)] = $obj[0];
		float $x = `rand $xPerametersLow $xPerametersHigh`;
		float $y = `rand $yPerametersLow $yPerametersHigh`;
		float $z = `rand $zPerametersLow $zPerametersHigh`;
		move $x $y $z;
	}
	
	move -r $xOffset $yOffset $zOffset $objects;
	group $objects;
}
}

RandomDuplicater();